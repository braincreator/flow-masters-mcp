# FlowMasters AI Agents - Environment Variables
# Copy to .env.local and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# AI Services URLs (FlowMasters Infrastructure)
QDRANT_URL=http://192.168.0.5:6333
QDRANT_API_KEY=your_qdrant_api_key
WEAVIATE_URL=http://192.168.0.5:8080
WEAVIATE_API_KEY=your_weaviate_api_key

# n8n Configuration
N8N_API_URL=https://n8n.flow-masters.ru/api
N8N_API_KEY=your_n8n_api_key

# Flowise Configuration  
FLOWISE_API_URL=https://flowise.flow-masters.ru/api
FLOWISE_API_KEY=your_flowise_api_key

# Crawl4AI Configuration
CRAWL4AI_API_URL=https://crawl.flow-masters.ru/api
CRAWL4AI_API_KEY=your_crawl4ai_api_key

# Agent Configuration
AGENTS_ENABLED=true
AGENTS_DEBUG=true
AGENTS_MAX_TOKENS=4000
AGENTS_TEMPERATURE=0.7
AGENTS_MAX_CONCURRENT=10

# Logging and Analytics
AGENTS_LOG_LEVEL=info
AGENTS_ANALYTICS_ENABLED=true

# Security
AGENTS_RATE_LIMIT=100
AGENTS_SESSION_TIMEOUT=3600