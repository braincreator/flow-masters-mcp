{
  "name": "flow-masters",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "node fix-importmap.js && cross-env NODE_OPTIONS='--max-old-space-size=8192' NEXT_JEST_WORKER_THREADS=false next build --no-lint && npm run generate:sitemap",
    "build:turbo": "node fix-importmap.js && cross-env NODE_OPTIONS='--max-old-space-size=8192' NEXT_JEST_WORKER_THREADS=false next build --turbopack --no-lint && npm run generate:sitemap",
    "analyze": "cross-env ANALYZE=true next build && npm run generate:sitemap",
    "build:prod": "node fix-importmap.js && cross-env NODE_ENV=production NEXT_JEST_WORKER_THREADS=false next build && npm run generate:sitemap",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "rebuild": "pnpm clean && pnpm install && pnpm build",
    "generate:sitemap": "node scripts/generate-sitemap.cjs",
    "postbuild": "npm run generate:sitemap",
    "dev": "node fix-importmap.js && cross-env NODE_OPTIONS='--max-old-space-size=4096 --no-deprecation' next dev",
    "dev:fast": "node fix-importmap.js && cross-env NODE_OPTIONS='--max-old-space-size=4096 --no-deprecation' next dev --turbopack",
    "dev:preview": "node fix-importmap.js && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "node scripts/fix-and-generate-types.mjs",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "start:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--no-deprecation next start",
    "start:standalone": "./start-standalone.sh",
    "generate-encryption-key": "node scripts/generate-encryption-key.mjs",
    "fill-config": "tsx scripts/fill-site-config.ts",
    "add-products": "tsx src/scripts/add-products-api.ts",
    "check-deps": "depcheck",
    "clean-deps": "pnpm store prune && pnpm prune",
    "clean-modules": "rm -rf node_modules && rm -rf .next && pnpm install",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:up": "cross-env NODE_OPTIONS=--no-deprecation payload migrate up",
    "migrate:down": "cross-env NODE_OPTIONS=--no-deprecation payload migrate down",
    "migrate:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "migrate:reset": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:reset",
    "sync:email-templates": "node src/scripts/run-sync-templates.js",
    "sync:email-templates:simple": "node src/scripts/simple-sync-templates.js",
    "create:email-templates": "node src/scripts/create-email-templates.js",
    "update:email-methods": "node src/scripts/update-email-methods.js",
    "test:email-templates": "node src/scripts/test-email-templates.js",
    "test:send-email": "tsx src/scripts/test-send-email.ts",
    "test:simple-email": "node src/scripts/simple-email-test.js",
    "test:enhanced-email": "tsx src/scripts/test-enhanced-email.js",
    "test:direct-email": "tsx src/scripts/direct-email-test.js",
    "test:email-service": "tsx src/scripts/email-service-test.js",
    "test:standalone-email": "node src/scripts/standalone-email-test.js",
    "migrate:services": "node src/scripts/migrate-services-simple.cjs"
  },
