{
  "name": "flow-masters",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' NEXT_JEST_WORKER_THREADS=false next build",
    "analyze": "cross-env ANALYZE=true next build",
    "build:prod": "cross-env NODE_ENV=production NEXT_JEST_WORKER_THREADS=false next build",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "rebuild": "pnpm clean && pnpm install && pnpm build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:fast": "cross-env NODE_OPTIONS=--no-deprecation NEXT_TURBO=1 next dev --turbo",
    "dev:preview": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "generate-encryption-key": "node scripts/generate-encryption-key.mjs",
    "fill-config": "tsx scripts/fill-site-config.ts",
    "add-products": "tsx src/scripts/add-products-api.ts",
    "check-deps": "depcheck",
    "clean-deps": "pnpm store prune && pnpm prune",
    "clean-modules": "rm -rf node_modules && rm -rf .next && pnpm install",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:up": "cross-env NODE_OPTIONS=--no-deprecation payload migrate up",
    "migrate:down": "cross-env NODE_OPTIONS=--no-deprecation payload migrate down",
    "migrate:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "migrate:reset": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:reset"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.0",
    "@payloadcms/db-mongodb": "^3.28.1",
    "@payloadcms/live-preview-react": "^3.28.1",
    "@payloadcms/next": "^3.28.1",
    "@payloadcms/payload-cloud": "^3.28.1",
    "@payloadcms/plugin-cloud-storage": "^3.28.1",
    "@payloadcms/plugin-form-builder": "^3.28.1",
    "@payloadcms/plugin-nested-docs": "^3.28.1",
    "@payloadcms/plugin-redirects": "^3.28.1",
    "@payloadcms/plugin-search": "^3.28.1",
    "@payloadcms/plugin-seo": "^3.28.1",
    "@payloadcms/richtext-lexical": "^3.28.1",
    "@payloadcms/storage-s3": "^3.28.1",
    "@payloadcms/ui": "^3.28.1",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-virtual": "^3.13.4",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "cross-env": "^7.0.3",
    "form-data": "^4.0.2",
    "framer-motion": "^11.18.2",
    "geist": "^1.3.1",
    "graphql": "^16.10.0",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.483.0",
    "next": "^15.2.3",
    "next-sitemap": "^4.2.3",
    "payload": "^3.28.1",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.54.2",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "sharp": "^0.33.5",
    "sonic-boom": "^4.2.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^7.5.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@next/bundle-analyzer": "^15.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.1.2",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.1.5",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
